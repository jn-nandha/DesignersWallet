<%= will_paginate users%>
<% users.each do |u| %>

	<div class="ibox float-e-margins container">
		<div class="ibox-content">
			<%= link_to user_profile_path(id: u.id) do %>
			<h3 class="font-bold"><%= u.name %></h3>
			<% end %>
				<% if @accepted.include?(u.id)%>
					<%= link_to "Unfollow", unfollow_path({to_id: u.id}) , method:"delete",class: "btn btn-outline btn-default"%>
				<% elsif @requested.include?(u.id)%>
					<%= link_to "cancel request",unfollow_path({to_id: u.id}) , method:"delete", class: "btn btn-outline btn-default"%>
				<%else %>
					<%= link_to "Follow", follows_path({to_id: u.id}), method: "post" ,class: "btn btn-outline btn-default"%>
				<% end %>
					<%= link_to "Block" , block_path({to_id: u.id}), method: "post", class: "btn btn-outline btn-default" %>
		</div>
	</div>
<% end %>

<%= will_paginate users%>




