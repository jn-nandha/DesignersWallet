<h2>Followers</h2> 
<% if @follower.count != 0 %>
  <% @follower.each do |f|%>
    <div class="ibox float-e-margins"> 
      <div class="ibox-content">
        <%= link_to user_profile_path(id: f.id) do %>
          <%= f.name %><br />
        <% end %>
        </div>
    </div>
  <% end %>
<% else %>
  <h3>NO Followers</h3>
<% end %>
