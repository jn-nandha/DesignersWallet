<h2>Followings</h2>

			<% if @following.count != 0 %>
				<% @following.each do |f|%>
				<div class="ibox float-e-margins">
					<div class="ibox-content">
	  				<%= link_to user_profile_path(id: f.id) do %>
						<%= f.name %>
					<% end %>
					</div>
				</div>

					
	 			<% end %>
			<% else %>
				<h3>NO Followings</h3>
			<% end %>
		