<div class="container" >
<h2 class="font-bold">Followings</h2>
<%= will_paginate @following %>
			<% if @following.count != 0 %>
				<% @following.each do |following|%>
  				<div class="ibox float-e-margins">
  					<div class="ibox-content">
  						<%= link_to user_profile_path(id: following.id) do %>
  						<%= following.name %>
  					  <% end %>
  					</div>
  				</div>
				<% end %>
			<% else %>
			<div class="ibox float-e-margins"> 
					<div class="ibox-content">
				<h3>NO Followings</h3>
				</div></div>
			<% end %>
<%= will_paginate @following %>
</div>