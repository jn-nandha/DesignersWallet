
<% @designs.each do |design| %>
<div class= 'col-sm-4 img-responsive' id= "did<%=design.id%>" >
	<%= link_to(image_tag("#{design.image_url}", size: "200x200"), home_path(design_id: design)) %>
	<% if @user == current_user %>
	<%= button_to "Delete image",designs_del_design_path(design_id: design.id),remote:true , method: "delete" ,class: "btn btn-default" %>
	<% end %>
	</div>
<% end %>
